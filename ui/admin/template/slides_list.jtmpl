<script type="text/x-jqote-template" id="template-list">
	<![CDATA[
	
	<% for (var i in this.records){ %>
	<a href="/admin/slides/<%= this.records[i].ID %>">
	<div class="col-xs-3" style="margin-bottom:20px;">
		
		<img src="/thumbnail/slides/243/120/<%= this.records[i].image_left %>" alt="">
		<h4 class="text-center"><%= this.records[i].heading %></h4>
		
	</div>
	</a>
	<% } %>
	
	]]>
</script>